    "next": "11.1.0",
    "next-urql": "^3.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "^1.38.1",
    "urql": "^2.0.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.2.0",
    "@graphql-codegen/typescript": "2.2.2",
    "@graphql-codegen/typescript-operations": "2.1.4",
    "@graphql-codegen/typescript-urql": "^3.2.0",
    "@types/react": "17.0.17",
    "concurrently": "^6.2.1",
    "eslint": "7.32.0",
